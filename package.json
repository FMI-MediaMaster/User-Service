{
  "packageManager": "pnpm@10.18.3",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc -b && tsc-alias && tsc-esm-fix --target dist",
    "prestart": "pnpm run build",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development pnpx tsx watch -r tsconfig-paths/register src/index.ts",
    "test": "NODE_ENV=test vitest --run",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "lint": "eslint_d src tests",
    "lint:fix": "eslint_d --fix src tests",
    "generate:database": "dotenv -e .env -- pnpx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" > src/database.types.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.8.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.38.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsc-esm-fix": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@media-master/express-crud-router": "^0.0.1",
    "@media-master/express-middleware": "^0.0.1",
    "@media-master/load-dotenv": "^0.0.1",
    "@supabase/supabase-js": "^2.77.0",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "zod": "^4.1.12"
  }
}
